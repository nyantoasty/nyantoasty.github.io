    "p": { "name": "Purl", "description": "Insert right needle through front of stitch from right to left, wrap yarn, push through.", "stitchesUsed": 1, "stitchesCreated": 1 },
    "yo": { "name": "Yarn Over", "description": "Wrap yarn over right needle to create an increase.", "stitchesUsed": 0, "stitchesCreated": 1 },
    "k2tog": { "name": "Knit 2 Together", "description": "Knit the next two stitches together as if they were one.", "stitchesUsed": 2, "stitchesCreated": 1 },
    "p2tog": { "name": "Purl 2 Together", "description": "Purl the next two stitches together as if they were one.", "stitchesUsed": 2, "stitchesCreated": 1 },
